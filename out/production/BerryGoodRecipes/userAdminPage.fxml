<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.TilePane?>


<BorderPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <top>
      <ToolBar nodeOrientation="RIGHT_TO_LEFT" prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
        <items>
          <Button fx:id="exitButton" mnemonicParsing="false" onAction="#onExitButtonAction" prefHeight="32.0" prefWidth="32.0" text="X" />
        </items>
      </ToolBar>
   </top>
   <left>
      <Pane prefHeight="560.0" prefWidth="480.0" BorderPane.alignment="CENTER">
         <children>
            <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="32.0" prefWidth="480.0" text="My Groups" />
            <TilePane fx:id="tilePane" layoutY="32.0" prefHeight="525.0" prefWidth="480.0" />
         </children>
      </Pane>
   </left>
   <right>
      <Pane prefHeight="556.0" prefWidth="320.0" BorderPane.alignment="CENTER">
         <children>
            <Label alignment="CENTER" prefHeight="32.0" prefWidth="320.0" text="Followed Users" />
            <ListView layoutY="32.0" prefHeight="525.0" prefWidth="320.0" />
         </children>
      </Pane>
   </right>
</BorderPane>
